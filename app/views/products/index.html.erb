<div class="container">
  <div class="row">

    <div class="text-center">
      <h1>Produkty</h1>
    </div>
  </div>
  <p id="notice"><%= notice %></p>

  <%= link_to "Dodaj produkt", new_product_path, class: "btn btn-primary" %>
